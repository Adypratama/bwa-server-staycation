   <!-- DataTales Example -->
   <div class="card shadow mb-4 mt-2">
    <div class="card-header py-3">
        <a href="/admin/booking" class="btn btn-info">Back</a>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-md-4">
                <img src="https://admin-funtraworld.herokuapp.com//<%= booking.payments.proofPayment %>"  alt="image" style="width: 100%; display: block;">
                <div class="row ml-1">
                    <form action="/admin/booking/<%= booking.id %>/confirmation?_method=PUT" method="POST">
                        <button type="submit" class="btn btn-primary btn-sm">
                            <i class="fas fa-trash"></i>
                            Confirmation
                        </button>
                    </form>
                    <form action="/admin/booking/<%= booking.id %>/reject?_method=PUT" method="POST" class="ml-3">
                        <button type="submit" class="btn btn-danger btn-sm">
                            <i class="fas fa-times"></i>
                            Reject
                        </button>
                    </form>
                </div>
            </div>
            <div class="col-md-8">
                <div class="table-responsive">
                    <table class="table table-bordered" id="dataTableActivity" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th class="text-center">Name Member</th>
                                <th class="text-center">Total</th>
                                <th class="text-center">Bank From</th>
                                <th class="text-center">Title</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                            <td><%= booking.memberId.firstName %> <%= booking.memberId.lastName %></td>
                            <td>$ <%= booking.total %></td>
                            <td><%= booking.payments.bankFrom %></td>
                            <td><%= booking.itemId.title %></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>